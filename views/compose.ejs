<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="entryTitle">Title</label>
        <textarea class="form-control" name="entryTitle" id="entryTitle" rows="1" cols="30"></textarea>
        <label for="entryContent">Post</label>
        <textarea class="form-control" name="entryContent" id="entryContent" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-primary type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>
